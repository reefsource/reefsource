{
  "family": "reefsource",
  "containerDefinitions": [
    {
      "image": "078097297037.dkr.ecr.us-east-1.amazonaws.com/reefsource:%IMAGE_TAG%",
      "name": "reefsource_web",
      "cpu": 384,
      "memory": 512,
      "essential": true,
      "entryPoint" : ["./bin/gunicorn.sh"],
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
